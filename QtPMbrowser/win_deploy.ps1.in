echo "===========> begin deploy"
$deploy_dir = "${CMAKE_CURRENT_BINARY_DIR}/deploy"
if (!(Test-Path $deploy_dir))
{
	echo "creating deploy dir"
	md $deploy_dir
}
echo "Calling ${WINDEPLOYQT_EXECUTABLE}"
${WINDEPLOYQT_EXECUTABLE} --verbose 0 --no-compiler-runtime --dir $deploy_dir "${CMAKE_CURRENT_BINARY_DIR}/QtPMbrowser.exe"
echo "<=========== finish deploy"
